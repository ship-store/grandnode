@model ViewModel
@{
    ViewBag.Title = T("Maintenance.EquipmentMaster").Text;
    var vessel = @HttpContextAccessor.HttpContext.Session.GetString("VesselName");
    //var equipment = @HttpContextAccessor.HttpContext.Session.GetString("Sub1_number");
     var ecode = ViewBag.code;
    var ename = ViewBag.name;
}

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="card">
        <div class="header">
            <h2>
                Add Job Plan
            </h2>
            <br />

            @*<ul class="header-dropdown m-r--5">
                <li class="dropdown">
