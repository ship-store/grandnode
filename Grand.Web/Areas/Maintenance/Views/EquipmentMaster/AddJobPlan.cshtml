                                <td><label for="NEXT_DUE_DATE">Next Due Date</label></td>
                                <td>
                                    <div class="form-group">
                                        <div class="form-line">
                                            <input type="date" class="form-control valid" name="NEXT_DUE_DATE" id="n4" />
                                        </div>
                                    </div>
                                </td>
                            </tr>

                            <tr>
                                @Html.AntiForgeryToken()
                                <td><input type="submit" class="btn btn-primary m-t-15 waves-effect" value="Submit" id="submit" /></td>
                            </tr>
                        </table>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        $("form").on('submit', function (e) {
            $(".valid").each(function () {

                var input = $(this);
                if (input.val() == "") {
                    swal($(this).attr("name") + " Cannot be blank");
                   e.preventDefault();
                    return false;
                }
                else {
                    swal({
                        title: "Success!",
                        text: "Submitting Form!",
                        icon: "success",
                        button: "Ok",
                    });
                }
            });
        });
    });
</script>