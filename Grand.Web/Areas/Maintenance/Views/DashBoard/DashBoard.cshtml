@{
    ViewBag.Title = T("Maintenance.Title.Success").Text;


}
<!DOCTYPE html>

@{
    var VesselName = @HttpContextAccessor.HttpContext.Session.GetString("VesselName");
    var SessionVar = @HttpContextAccessor.HttpContext.Session.GetString("email");

    if (VesselName == null)
    {
        <script>

        $(window).load(function(){
            swal({

                text: 'Please select a Vessel',
                allowOutsideClick: false,
                background: 'white',
                confirmButtonText:'OK',
                confirmButtonColor: '#0f5f86',

            })
                .then(function () {
                    var url1 = location.origin + "Maintenance/DueJob/ReadDueJob";
                    $.ajax({
                            type: "GET",
    	                        url:"http://localhost:16599/Maintenance/DueJob/ReadDueJob",
    	                        success: function(data){ 
                                    // $('#data').text(data);
                                    alert("Haiu");

    	                        },
    	                        error: function(){
    		                        alert("There was an error.");
    	                        }
                            });


                    window.location.href = '@Url.Action("VesselList","Vessel")';
                })

            });

        </script>
    }

    if (SessionVar == null)
    {
        <script>

            $(window).load(function ()
            {
            swal({

                title: 'TimeOut!',
                allowOutsideClick: false,
                text: '"Time Out! , Please Login"',
                background: 'white',
                confirmButtonText: 'OK',
                confirmButtonColor: '#48b4e0',
            })
                .then(function ()
                {
                    window.location.href = '@Url.Action("LoginPage","Register")';
                })

            });

        </script>
    }

}


<html>
<head>
    <meta name=" viewport" content="width=device-width" />
    <title>Success</title>
</head>

<body>

    <section class="content">
        <div class="container-fluid">
            <div class="block-header">
                <h2>STATISTICS</h2>
            </div>

            <!-- Widgets -->
            <div class="row clearfix">
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-pink hover-expand-effect">
                        <div class="icon">
                            <i class="fa fa-ship"></i>
                        </div>
                        <div class="content">
                            <div class="text">TOTAL VESSELS</div>
                            <div class="text">450</div>
                            <div class="number count-to" data-from="0" data-to="000" data-speed="15" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-cyan hover-expand-effect">
                        <div class="icon">
                            <i class="icon-calendar"></i>
                        </div>
                        <div class="content">
                            <div class="text">DUE FOR THE DAY</div>
                            <div class="text">2</div>
                            <div class="number count-to" data-from="0" data-to="000" data-speed="1000" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-light-green hover-expand-effect">
                        <div class="icon">
                            <i class="icon-calendar"></i>
                        </div>
                        <div class="content">
                            <div class="text">DUE FOR THE WEEK</div>
                            <div class="text">5</div>
                            <div class="number count-to" data-from="0" data-to="000" data-speed="1000" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                    <div class="info-box bg-orange hover-expand-effect">
                        <div class="icon">
                            <i class="icon-calendar"></i>
                        </div>
                        <div class="content">
                            <div class="text">DUE FOR THE MONTH</div>
                            <div class="text">8</div>
                            <div class="number count-to" data-from="0" data-to="000" data-speed="1000" data-fresh-interval="20"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
